{
	"name": "Sploit",
	"version": "2.0.0",
	"description": "Sploit loader",
	"manifest_version": 2,
	"permissions": [
		"webNavigation",
		"downloads",
		"https://krunker.io/*",
		"https://comp.krunker.io/*",
		"webRequest",
		"webRequestBlocking"
	],
	"browser_action": {
		"default_title": "Sploit",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["js/bg.js"],
		"persistent": true
	},
	"icons": {
		"48": "icon.png"
	}
}